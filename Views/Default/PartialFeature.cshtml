@model List<AcunMedyaPortfolio.Models.TblFeature>
<section id="home-section" class="hero">
    <div class="home-slider owl-carousel">
        @foreach (var item in Model)
        {
            <div class="slider-item">
                <div class="overlay"></div>
                <div class="container-fluid px-md-0">
                    <div class="row d-md-flex no-gutters slider-text align-items-end justify-content-end" data-scrollax-parent="true">
                        <div class="one-third order-md-last img" style="background-image: url('/Template/clyde-master/images/IMG-20240316-WA0043.jpg');">
                            <div class="overlay"></div>
                            <div class="overlay-1"></div>
                        </div>
                        <div class="one-forth d-flex  align-items-center ftco-animate" data-scrollax=" properties: { translateY: '70%' }">
                            <div class="text">
                                <span class="subheading">@item.FeatureSubtitle</span>
                                <h1 class="mb-4 mt-3">@item.FeatureTitle</h1>
                                <p><a href="https://www.linkedin.com/in/%C3%A7a%C4%9Fr%C4%B1-osman-ediz-264550258/" class="btn btn-primary btn-outline-primary">CV İndir </a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>



        }





    </div>
</section>

