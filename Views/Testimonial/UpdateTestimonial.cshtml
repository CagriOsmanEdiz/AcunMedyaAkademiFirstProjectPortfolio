@model AcunMedyaPortfolio.Models.TBlTestimonial
@{
    ViewBag.Title = "UpdateTestimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h2>Yeni Referans Güncelleme</h2>
<form method="post">
    <label>Referans Id</label>
    @Html.TextBoxFor(x => x.TestimonialId, new { @class = "form-control" })
    <br />
    <label>Referans İsmi</label>
    @Html.TextBoxFor(x => x.TestimonialName, new { @class = "form-control" })
    <br />

    <label>Referans Mesleği</label>
    @Html.TextBoxFor(x => x.TestimonialName, new { @class = "form-control" })
    <br />

    <label>Referans Açıklaması</label>
    @Html.TextBoxFor(x => x.TestimonialDescription, new { @class = "form-control" })
    <br />

    <label>Referans Resmi</label>
    @Html.TextBoxFor(x => x.TestimonialmageUrl, new { @class = "form-control" })
    <br />
    <button class="btn-btn-info>">Referansı Güncelle </button>
</form>
